.topsCircle {
  width: 740px;
  margin: 0 auto;
  margin-top: 35px;
}
.topsCircle .row {
  margin: 0;
}
.topsCircle .circlesUl {
  width: 510px;
  margin: 0 auto;
}
.topsCircle .circlesUl .circlesLi {
  margin: 25px;
  float: left;
}
.topsCircle .circlesUl .circlesLi .circle {
  position: relative;
  width: 120px;
  height: 120px;
  -webkit-border-radius: 100%;
  -moz-border-radius: 100%;
  border-radius: 100%;
  background-image: url(../img/circles.png);
  background-repeat: no-repeat;
  background-position: -2px -2px;
  color: #5a6a7a;
}
.topsCircle .circlesUl .circlesLi .circle .blue {
  color: #36b0c8;
}
.topsCircle .circlesUl .circlesLi .circle .green {
  color: #1dae63;
}
.topsCircle .circlesUl .circlesLi .circle .purple {
  color: #51599f;
}
.topsCircle .circlesUl .circlesLi .circle .toptitle {
  padding-top: 20px;
}
.topsCircle .circlesUl .circlesLi .circle .bigfont {
  font-size: 40px;
}
.topsCircle .circlesUl .circlesLi .green-linear {
  background-position: -126px -2px;
}
.topsCircle .circlesUl .circlesLi .purple-linear {
  background-position: -250px -2px;
}
.btns,
.lists {
  width: 740px;
  margin: 0 auto;
}
.btns .row,
.lists .row {
  margin: 0;
}
.btns {
  margin-top: 30px;
}
.btns .btns-ul {
  display: inline-block;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  overflow: hidden;
  border: #cfcfcf 1px solid;
}
.btns .btns-ul .btns-li {
  float: left;
  border-right: #cfcfcf 1px solid;
}
.btns .btns-ul .btns-li:last-child {
  border-right: none;
}
.btns .btns-ul .btns-li .btns-btn {
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  color: #5a6a7a;
  text-align: center;
  padding: 0 16px;
  outline: none;
  border: none;
  background: #fff;
}
.btns .btns-ul .btns-li .btns-btn.active {
  background: #39adc4;
  color: #fff;
}
.lists .no-record {
  font-size: 30px;
  text-align: center;
  margin-top: 60px;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  background: #fff;
  height: 80px;
  line-height: 80px;
  border: #cfcfcf 1px solid;
}
.lists .time {
  color: #5a6a7a;
}
.lists .time .yyyy {
  font-size: 16px;
}
.lists .time .mmdd {
  font-size: 26px;
  font-weight: bold;
}
.lists .comment-li {
  background: #fff;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  border: #cfcfcf 1px solid;
  padding: 25px 36px;
  margin-top: 12px;
}
.lists .comment-li .top-part .left-part,
.lists .comment-li .top-part .center-part,
.lists .comment-li .top-part .right-part {
  float: left;
}
.lists .comment-li .top-part .left-part .imgpart {
  width: 70px;
  height: 70px;
  -webkit-border-radius: 100%;
  -moz-border-radius: 100%;
  border-radius: 100%;
  background-image: url(../img/defaultIco.png);
  background-size: 100% 100%;
}
