.topsCircle .circlesUl .circlesLi {
  margin: 20px;
  float: left;
}
.topsCircle .circlesUl .circlesLi .circle {
  width: 120px;
  height: 120px;
  border-width: 12px;
  border-style: solid;
  -webkit-border-radius: 100%;
  -moz-border-radius: 100%;
  border-radius: 100%;
}
.topsCircle .circlesUl .circlesLi .blue-linear {
  border-color: #36b1c8;
}
.topsCircle .circlesUl .circlesLi .green-linear {
  border-color: #1faf62;
  border: #1eae63  #52ca4f 30px solid;
}
.topsCircle .circlesUl .circlesLi .purple-linear {
  border-color: #535aa0;
}
